NSTRING RStrncpy(STRING src, STRING dst, INT32U num)
Xchar* RStrncpy(char* src, char* dst, INT32U num)
N{
N	
N	STRING src_cpy = src;
X	char* src_cpy = src;
N
N	if ( src == STRING_NULL || dst == STRING_NULL )
