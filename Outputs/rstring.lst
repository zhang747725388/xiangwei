Nvoid ByteToStr(BYTE Src , BYTE * Dst)
N{
N	BYTE Src1;
N	BYTE Src2;
N	Src1 = ( Src >> 4 ) & 0x0F;
N	Src2 = Src & 0x0F;
N	if( ( Src1 >= 0x0A ) && (Src1 <= 0x0F) ){
N		*Dst = Src1+0x37;
N	}else if(( Src1 >= 0x01 ) && (Src1 <= 0x09)){
N		*Dst = Src1+0x30;
N	}else{
N		*Dst = 0x30;
N	}
N
N	Dst++;
N
N	if( ( Src2 >= 0x0A ) && (Src2 <= 0x0F) ){
N		*Dst = Src2+0x37;
N	}else if(( Src2 >= 0x01 ) && (Src2 <= 0x09)){
N		*Dst = Src2+0x30;
N	}else{
N		*Dst = 0x30;
N	}
N}
